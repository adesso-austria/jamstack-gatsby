{
  "connection": "default",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": "my personal blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "maxLength": 25,
      "required": true,
      "minLength": 3,
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "published": {
      "required": true,
      "type": "boolean"
    }
  }
}